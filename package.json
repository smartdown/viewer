{
  "name": "smartdown-viewer",
  "version": "0.0.50",
  "description": "",
  "main": "index.js",
  "scripts": {
    "rmdsstore": "find . -name '.DS_Store' -print -delete",
    "clean": "rm -rf dist/",
    "dupindex": "cp dist/index.html dist/404.html",
    "analyze": "npm run clean && BUILD=analyze node_modules/.bin/webpack --bail --progress --colors",
    "build": "npm run clean && BUILD=production node_modules/.bin/webpack -p --verbose --bail --progress --colors",
    "dev": "BUILD=development node_modules/.bin/webpack-dev-server --host localhost --port 4001 --devtool=source-map --progress --colors --content-base app",
    "devbuild": "npm run build && (cd dist && ln -s . viewer) && npm run server",
    "ghpublish": "./publish.sh",
    "server": "node_modules/.bin/http-server --cors -c-1 dist/",
    "test": "export BUILD=test && karma start"
  },
  "author": "Daniel Keith <dan@quantumclay.com> (https://doctorbud.com)",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.7.9",
    "angular-resource": "^1.7.9",
    "bootstrap": "^4.5.0",
    "codemirror": "^5.54.0",
    "ng-file-upload": "^12.2.13",
    "smartdown": "^1.0.47",
    "smartdown-gallery": "0.0.71",
    "split.js": "^1.5.11",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-umd": "7.9.0",
    "@babel/preset-env": "7.9.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-html": "^6.0.2",
    "exports-loader": "^0.7.0",
    "expose-loader": "^0.7.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^4.2.0",
    "font-awesome": "^4.7.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.3",
    "imports-loader": "^0.8.0",
    "json-loader": "^0.5.7",
    "raw-loader": "^3.1.0",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "ui-bootstrap4": "^3.0.6",
    "url-loader": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
